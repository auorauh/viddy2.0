{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev & npm run dev:server",
    "dev": "vite",
    "dev:server": "tsx watch src/server/server.ts",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:server": "vitest run src/server/__tests__",
    "test:db": "node --loader ts-node/esm src/lib/database/test-connection.ts",
    "test:unit": "vitest run --reporter=verbose src/**/*.test.ts --exclude=src/**/*.integration.test.ts --exclude=src/**/*.e2e.test.ts --exclude=src/**/*.performance.test.ts",
    "test:integration": "vitest run --reporter=verbose src/**/*.integration.test.ts",
    "test:e2e": "vitest run --reporter=verbose src/**/*.e2e.test.ts",
    "test:performance": "vitest run --reporter=verbose src/**/*.performance.test.ts",
    "test:load": "vitest run --reporter=verbose src/**/*load*.test.ts",
    "test:all": "vitest run --reporter=verbose",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --reporter=verbose",
    "debug:project": "tsx src/scripts/debug-project.ts",
    "test:project-flow": "tsx src/scripts/test-project-flow.ts"
  },
